    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "eslint": "^9.33.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.14.0"
