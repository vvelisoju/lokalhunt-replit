{
  "appId": "com.codevel.lokalhunt",
  "appName": "LokalHunt",
  "webDir": "client/dist",
  "plugins": {
    "SplashScreen": {
      "launchAutoHide": true,
      "androidScaleType": "CENTER_CROP",
      "showFullscreen": false
    },
    "StatusBar": {
      "style": "LIGHT_CONTENT",
      "backgroundColor": "#1976d2",
      "overlaysWebView": false
    }
  },
  "server": {
    "androidScheme": "https"
  }
}
